{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://tokobrut.vercel.app"
    }
}